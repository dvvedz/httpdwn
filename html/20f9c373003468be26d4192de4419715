<p>
<div class="content">
<iframe id="haufe-iframe" src="https://recruitingapp-5129.de.umantis.com/Jobs/All?lang=ger&amp;DesignID=10006" width="100%" height="4500px" seamless="seamless" frameborder=0></iframe><script type="text/javascript">
// Here "addEventListener" is for standards-compliant web browsers and "attachEvent" is for IE Browsers.
var eventMethod = window.addEventListener ? "addEventListener" : "attachEvent";
var eventer = window[eventMethod];
// Now...
// if
// "attachEvent", then we need to select "onmessage" as the event.
// if
// "addEventListener", then we need to select "message" as the event
var messageEvent = eventMethod == "attachEvent" ? "onmessage" : "message";
// Listen to message from child IFrame window
eventer(messageEvent, function (e) {
// Do whatever you want to do with the data got from IFrame in Parent form.
if(e.data.indexOf("resize-") !== -1){
jQuery("#haufe-iframe").attr("height", parseInt(e.data.replace("resize-","")) + 150 +"px");
}
if(e.data == "tracking-apply-click"){
ga("ev_com.send", "event", "career", "application", "contact_sent",1);
ga("landingpage.send", "event", "career", "landingpages", "contact_sent",1);
}
}, false);
</script>
