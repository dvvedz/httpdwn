<script type="text/javascript">
function getLicencePartner() {
return '69e64fc5-bc1f-47a1-b172-b3749f13a888'
}
$(window).on('load', function () {
if (!evm.cookies.isInCookieConsent(getLicencePartner()) && !evm.cookies.isInCookieNoConsent(getLicencePartner())) {
fdc('cookieoptin_view', null, {
screen_resolution: window.screen.width + 'x' + window.screen.height,
language: window.navigator.language,
referrer: document.referrer,
});
$('#cookieConsentDialog').modal('show');
// bug on firefox: sometimes .modal('show') is not adding the styling to the modal even though the event is firing correctly
$('#cookieConsentDialog').css('display', 'flex');
$('#cookieConsentDialog').addClass('in');
evm.util.blockDocumentScrolling();
// show more button visibility handling
var textWrapper = document.getElementById('cookieConsentTextWrapper');
var text = document.getElementById('cookieConsentText');
if (text.clientHeight <= textWrapper.clientHeight) {
document.getElementById('cookieConsentShowMoreBtn').style.display = 'none';
}
$('#cookieConsentDialog').on('hidden.bs.modal', function () {
evm.util.restoreDocumentScrolling();
});
}
if (evm.cookies.isInCookieConsent(getLicencePartner())) {
evm.cookies.triggerCookieConsentGTMEvent()
}
});
</script>
