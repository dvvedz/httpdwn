var previewContentRoot = $('#ev-search-preview-results');
// Add Last Searched entry for location preview item:
previewContentRoot.on('click', 'a.ev-search-item-link.location', function(e) {
var url = $(this).attr('href');
var offerTypeDisplayValue = fullScreenSearchApi.getSelectedOfferType().displayValue;
var objectTypeDisplayValue = fullScreenSearchApi.getSelectedObjectType().displayValue;
var displayText =
offerTypeDisplayValue + ', ' +
objectTypeDisplayValue + ', ' +
$(this).find('.ev-preview-item-text').text();
lastSearched.addLastSearched(displayText, url, 'location-item');
});
// Add Last Searched entry for property preview item:
previewContentRoot.on('click', 'a.ev-search-item-link.property', function(e) {
var url = $(this).attr('href');
var displayText = $(this).find('.ev-preview-item-text').text();
var previewImageUrl = $(this).data('preview-image-url');
lastSearched.addLastSearched(displayText, url, 'property-item', previewImageUrl);
});
</script>
