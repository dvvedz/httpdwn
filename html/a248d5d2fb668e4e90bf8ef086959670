<link rel="stylesheet" type="text/css" media="all" href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.2.0/assets/owl.carousel.min.css" />
<link rel="stylesheet" type="text/css" media="all" href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.2.0/assets/owl.theme.default.min.css" />
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.2.0/owl.carousel.min.js"></script>
<script type="text/javascript">
$(document).ready(function(){
$("#owl-thumbs").owlCarousel({
startPosition: 1,
responsiveClass:true,
items: 3,
singleItem: true,
dots: false,
responsive: {
0:{
items:1
},
780:{
items: 1
},
1260:{
items: 1
}
},
onDragged: callback
});
$("#owl-thumbs").on('change.owl.carousel', function(event) {
if (event.namespace && event.property.name === 'position') {
var target = event.relatedTarget.relative(event.property.value, true);
$("#owl-paragraph").owlCarousel('to', target, 300, true);
}
});
$("#owl-paragraph").owlCarousel({
startPosition: 1,
nav: false,
responsiveClass:true,
items: 3,
singleItem: true,
dots: false,
mouseDrag: false,
touchDrag: false,
responsive: {
0:{
items:1
},
780:{
items: 1
},
1260:{
items: 1
}
},
});
// Activate certain thumb on dragging
function callback(event) {
var item = event.item.index+1;
$('.owl-thumbs a').removeClass('active');
$('.owl-thumbs a:nth-child('+item+')').addClass('active');
var positionTriangle = $('.owl-thumbs a:nth-child('+item+')').position();
if ($( window ).width() <= 361) {
$(".triangle").animate({left: positionTriangle.left+20});
} else if ($( window ).width() <= 540) {
$(".triangle").animate({left: positionTriangle.left+30});
} else {
$(".triangle").animate({left: positionTriangle.left+45});
}
}
$('.owl-thumbs a').click(function(){
$('.owl-thumbs a').removeClass('active');
$(this).addClass('active');
var positionTriangle = $(this).position();
// Animate triange on click of thumb
if ($( window ).width() <= 361) {
$(".triangle").animate({left: positionTriangle.left+20});
} else if ($( window ).width() <= 540) {
$(".triangle").animate({left: positionTriangle.left+30});
} else {
$(".triangle").animate({left: positionTriangle.left+45});
}
});
});
</script>
