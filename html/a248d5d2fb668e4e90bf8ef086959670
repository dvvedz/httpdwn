<script type="text/javascript">
$('#us_query').on('focus', function() {
function encodeHTML(unencodedStr) {
const div = document.createElement('div');
div.appendChild(document.createTextNode(unencodedStr));
return div.innerHTML;
}
// Do not trigger "last searched" suggestions if the input isn't empty:
if ($(this).val()) {
return;
}
var lastSearchedItems = lastSearched.getLastSearched();
if (lastSearchedItems.length === 0) {
return;
}
var lastSearchedContent =
'<div class="ev-preview-content-header">Letzte Suchen</div>';
if (us) {
us.visibleResultType = "storedSearch";
us.visibleResults = lastSearchedItems.length;
}
lastSearchedItems.forEach(function(lastSearchedItem, i) {
url = lastSearchedItem.url;
displayText = lastSearchedItem.displayText;
var iconClassName = '';
var iconStyle = ''
switch (lastSearchedItem.category) {
case 'location-item':
iconClassName = 'icon-location';
break;
case 'fulltext-search':
iconClassName = 'icon-fulltext';
break;
case 'property-item':
iconClassName = 'icon-property';
iconStyle = 'background-image: url(' + lastSearchedItem.itemImageUrl + ');';
break;
}
lastSearchedContent +=
'<a class="ev-search-item-link" href="' + url + '" onclick="if(us){us.trackStoredSearchSelected(\'' + url + '\', \'' + lastSearchedItem.category + '\', ' + lastSearchedItems.length + ')}">' +
'<div class="ev-preview-content-item">' +
'<div class="ev-preview-item-icon ' + iconClassName + '" style="' + iconStyle + '">' +
'</div>' +
'<div class="ev-preview-item-text"><span class="ev-preview-item-text-emphasize">' + encodeHTML(displayText) + '</span></div>' +
'</div>' +
'</a>';
});
us.setSuggestionItems(lastSearchedContent);
});
</script>
