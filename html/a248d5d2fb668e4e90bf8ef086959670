<script type="text/javascript">
// set the height of the fullscreen keyvisual to the full screen height on mobile devices
function resizeFullscreenKeyVisual() {
var $keyVisual = $('.keyvisual-fullscreen');
if (evm.isMobile()) {
var windowHeight = $(window).height();
var headerHeight = $('.header-optionally-fixed').height() + $('.header-bottom.primary-menu').height();
// Some browsers, e.g. iPhone 5 Safari, ignore the min-height, when height is set. Hence, we have to
// make sure to set the keyvisual height to something bigger than the keyvisual's min-height attribute.
// However, we only get the min-height as a string in the form of '635px'.
var keyVisualMinHeightStr = $keyVisual.css('min-height');
var keyVisualMinHeight;
if (keyVisualMinHeightStr.indexOf("px") >= 0) {
keyVisualMinHeight = parseInt(keyVisualMinHeightStr.substring(0,keyVisualMinHeightStr.length-2));
}
var newKeyVisualFullscreenHeight = windowHeight - headerHeight;
if (keyVisualMinHeight !== undefined && newKeyVisualFullscreenHeight < keyVisualMinHeight) {
newKeyVisualFullscreenHeight = keyVisualMinHeight
}
$keyVisual.height( newKeyVisualFullscreenHeight );
} else {
$keyVisual.height( '' );
}
}
$(window).on('resize', resizeFullscreenKeyVisual );
$(document).ready( resizeFullscreenKeyVisual );
</script>
</header>
