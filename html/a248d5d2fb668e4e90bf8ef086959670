<div class="keyvisual keyvisual-fullscreen">
<script type="text/javascript">
function calcWidth() {
/* Wir berechnen die Breite des Screens und beachten dabei die devicePixelRatio für Retina-Support.
* Die Pixel-Ratio beschränken wir dabei auf 1..2, um die Datenmenge nicht unnötig zu erhöhen.
*/
var winWidth = $(window).width() * Math.min(2, Math.max(1, window.devicePixelRatio) );
var screensizes = [[768,240],[960,300],[1024,320],[1280,400]];
var c = screensizes.length;
for (var i = 0; i < c; i++) {
if (screensizes[i][0]>=winWidth) {
return screensizes[i];
}
}
return screensizes[c - 1];
}
var resizeWidth=calcWidth();
</script>
<div class="keyvisual-fullscreen-image"></div>
<div class="hide-none-phone">
</div>
<script type="text/javascript">
(function () {
var images = jQuery('.keyvisual .keyvisual-slider > div');
var mobileTeasers = jQuery('.keyvisual-teaser-part-mobile-view');
var switch_image_interval = null;
var switch_image_duration = 6000;
var change_opacity_duration = 3000;
function switch_element(old_element, new_element) {
old_element.fadeOut(change_opacity_duration);
new_element.fadeIn(change_opacity_duration);
}
if (images.size()>1) {
images.hide().css('position', 'absolute').css('top', 0).css('left', '0').first().css('position', 'relative').addClass('current').show();
switch_image_interval = setInterval(function () {
// Switch images
var currentImage = images.filter('.current').removeClass('current').css('position', 'absolute');
var firstImage = images.filter(':first');
var lastImage = images.filter(':last');
var nextImage = currentImage.nextAll('div').first();
if (images.index(currentImage) == images.index(lastImage)) {
nextImage = firstImage;
}
nextImage.addClass('current').css('position', 'relative');
switch_element(currentImage, nextImage);
// Switch mobile teasers
var currentMobileTeaser = mobileTeasers.filter('.current').removeClass('current');
var firstMobileTeaser = mobileTeasers.filter(':first');
var lastMobileTeaser = mobileTeasers.filter(':last');
var nextMobileTeaser = currentMobileTeaser.nextAll('div').first();
if (mobileTeasers.index(currentMobileTeaser) == mobileTeasers.index(lastMobileTeaser)) {
nextMobileTeaser = firstMobileTeaser;
}
nextMobileTeaser.addClass('current');
switch_element(currentMobileTeaser, nextMobileTeaser);
}, switch_image_duration);
}
})();
</script>
