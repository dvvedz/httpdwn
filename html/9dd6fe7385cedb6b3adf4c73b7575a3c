        /**
         * Sets the decimal number format symbols for $locale
         * @param locale $locale service
         */
        var setDecimalFormatProperties = function(locale) {
            locale.NUMBER_FORMATS.DECIMAL_SEP = ".";
            locale.NUMBER_FORMATS.GROUP_SEP = ",";
        };

