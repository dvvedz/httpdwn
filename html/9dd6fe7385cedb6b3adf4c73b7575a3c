        var performCourseOperation = function (bookingTypeId, operation, courseId, parentCourseId, parentCourseSetId) {
            if (operation !== "cancel") {
                var targetPath = '/pages/coursedescription.jsf';
                var query = "?bookingTypeId=" + bookingTypeId;
