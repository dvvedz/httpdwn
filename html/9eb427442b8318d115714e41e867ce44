		<script type="text/javascript">
		    function init_cookie_message(cookie_name, days) {
                cookie_name = cookie_name || 'accept_cookies';
                days = days || 3650;

                if(document.cookie.indexOf(cookie_name + "=") >= 0) {
                    return;
                }

                var container = document.getElementById('cookies-text');
                container.style.display = "block";

                var close = document.getElementById('cookies-close');
                close.onclick = function() {
                    container.style.display = "none";
                };

                var accept = document.getElementById('cookies-accept');
                accept.onclick = function() {
                    container.style.display = "none";

                    var d = new Date();
                    d.setTime(d.getTime() + (days*24*60*60*1000));
                    document.cookie = cookie_name + "=1;expires=" + d.toUTCString() + ";path=/";
                };
		    }
		    init_cookie_message();
		</script>
